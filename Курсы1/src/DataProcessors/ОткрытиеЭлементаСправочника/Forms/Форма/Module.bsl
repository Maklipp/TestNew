Процедура ОткрытьЭлемент(Команда)

	СтруктураПараметры = Новый Структура();
	СтруктураПараметры.Вставить("Ключ", СправочникСсылка);
	СтруктураПараметры.Вставить("ПереданнаяИнформация", "Информация из формы, которую мы передали");
	
	ОткрытьФорму("Справочник.Номенклатура.ФормаОбъекта", СтруктураПараметры);
	
